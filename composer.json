{
    "name": "rishadblack/oracle-table-linker",
    "description": "A Laravel package for Oracle database users, allowing seamless management of database-linked tables and aliases with automated first-call handling and dynamic aliasing.",
    "license": "MIT",
    "authors": [{
        "name": "S M Rishad",
        "email": "rishad.black@gmail.com",
        "homepage": "https://smrishad.com"
    }],
    "homepage": "https://github.com/rishadblack/oracle-table-linker",
    "keywords": ["Laravel", "OracleTableLinker"],
    "version": "1.1.3",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Rishadblack\\OracleTableLinker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rishadblack\\OracleTableLinker\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rishadblack\\OracleTableLinker\\OracleTableLinkerServiceProvider"
            ],
            "aliases": {
                "OracleTableLinker": "Rishadblack\\OracleTableLinker\\Facades\\OracleTableLinker"
            }
        }
    }
}
